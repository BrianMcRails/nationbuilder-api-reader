<h1 style="font-size: 3em; color: darkgreen; text-align: center; font-family: Courier;">Listing nb_users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Lastname</th>
      <th>Firstname</th>
      <th>Emailoptin</th>
      <th>Updatedat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nb_users.each do |nb_user| %>
      <tr>
        <td><%= nb_user.email %></td>
        <td><%= nb_user.lastname %></td>
        <td><%= nb_user.firstname %></td>
        <td><%= nb_user.emailoptin %></td>
        <td><%= nb_user.updatedat %></td>
        <td><%= link_to 'Show', nb_user %></td>
        <td><%= link_to 'Edit', edit_nb_user_path(nb_user) %></td>
        <td><%= link_to 'Destroy', nb_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<p><%= @result %></p>
